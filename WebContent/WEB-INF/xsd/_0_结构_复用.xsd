      <xsd:element name="审委会委员到庭听案"       type="TN"  minOccurs="0"/>
      <xsd:element name="宣判情况"                 type="TN"  minOccurs="0"/>
      <xsd:element name="笔录"                     type="SN"  minOccurs="0"/>
      <xsd:element name="听证裁决情况"             type="TN"  minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="法庭使用记录_强医">
    <xsd:sequence>
      <xsd:element name="R" type="法庭使用记录_强医1" minOccurs="0" maxOccurs="32768"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="证据信息1">
