      <xsd:element name="原物原件"   type="TN"   minOccurs="0"/>
      <xsd:element name="取证时间"   type="DT"   minOccurs="0"/>
      <xsd:element name="取证地点"   type="C100" minOccurs="0"/>
      <xsd:element name="取证渠道"   type="TN"   minOccurs="0"/>
      <xsd:element name="举证人"     type="BB"   minOccurs="0"/>
      <xsd:element name="举证日期"   type="D"    minOccurs="0"/>
      <xsd:element name="收证人"     type="SN"   minOccurs="0"/>
      <xsd:element name="有利方"     type="BB"   minOccurs="0"/>
      <xsd:element name="证据事实"   type="C200" minOccurs="0"/>
      <xsd:element name="委托鉴定"   type="TN"   minOccurs="0"/>
