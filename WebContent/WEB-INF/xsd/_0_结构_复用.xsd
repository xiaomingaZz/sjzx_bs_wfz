  <xsd:complexType name="鉴定信息1">
    <xsd:sequence>
      <xsd:element name="序号_鉴定"      type="SN"   minOccurs="1"/>
      <xsd:element name="证据序号"       type="SN"   minOccurs="0"/>
      <xsd:element name="重新鉴定"       type="TN"   minOccurs="0"/>
      <xsd:element name="重新鉴定理由"   type="N"    minOccurs="0"/>
      <xsd:element name="委托鉴定人"     type="SN"   minOccurs="0"/>
      <xsd:element name="当事人委托"     type="TN"   minOccurs="0"/>
      <xsd:element name="委托鉴定内容"   type="C240" minOccurs="0"/>
