  </xsd:complexType>
  <xsd:complexType name="证据链条">
    <xsd:sequence>
      <xsd:element name="R" type="证据链条1" minOccurs="0" maxOccurs="32768"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="链条中的证据1">
    <xsd:sequence>
      <xsd:element name="证据链"       type="SN" minOccurs="1"/>
      <xsd:element name="证据"         type="SN" minOccurs="1"/>
      <xsd:element name="顺序号"       type="SN" minOccurs="0"/>
      <xsd:element name="再现事实时间" type="DT" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="链条中的证据">
    <xsd:sequence>
      <xsd:element name="R" type="链条中的证据1" minOccurs="0" maxOccurs="32768"/>
