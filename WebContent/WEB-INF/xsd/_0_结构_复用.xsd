<?xml version="1.0" encoding="GB2312"?>
<xsd:schema  xmlns="http://dataexchange.court.gov.cn/2009/data"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   targetNamespace="http://dataexchange.court.gov.cn/2009/data"
elementFormDefault="qualified">

  <xsd:include schemaLocation="_A_类型_基础.xsd"/>

  <xsd:simpleType name="案件标识">
    <xsd:restriction base="xsd:nonNegativeInteger">
      <xsd:totalDigits value="15"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:group name="自然人基本状况">
    <xsd:sequence>
      <xsd:element name="性别"               type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="出生日期"           type="D"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="年龄"               type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="国籍"               type="SN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="民族"               type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="身份"               type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="身份证号码"         type="C30"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="现役军人身份"       type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="军官职务性质"       type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="军官或文职干部级别" type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="其他身份证件种类"   type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="其他身份证件号码"   type="C30"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="职业"               type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="文化程度"           type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="婚姻状况"           type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="政治面貌"           type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="行政级别"           type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="所在单位"           type="C100" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="职务"               type="C100" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="特殊身份"           type="N"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="特殊生理或病理"     type="N"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="户籍所在地"         type="C100" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="简历"               type="BB"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="流动人员"           type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="未成年人家庭背景"   type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="家庭经济状况"       type="TN"   minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:group name="单位基本状况">
    <xsd:sequence>
      <xsd:element name="国家或地区"     type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="组织机构代码"   type="C10" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="单位性质"       type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="特殊行业"       type="N"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="法定代表人"     type="C40" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="代表人证件种类" type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="代表人证件号码" type="C30" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="与案实体">
    <xsd:sequence>
      <xsd:element name="序号" type="SN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="来源" type="TN"   minOccurs="1" maxOccurs="1"/>
      <xsd:element name="标识" type="N"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="角色" type="BB"   minOccurs="1" maxOccurs="1"/>
      <xsd:element name="类型" type="TN"   minOccurs="1" maxOccurs="1"/>
      <xsd:element name="名称" type="C100" minOccurs="0" maxOccurs="1"/>
      <xsd:choice>
        <xsd:group ref="自然人基本状况"/>
        <xsd:group ref="单位基本状况"/>
      </xsd:choice>
      <xsd:element name="地址"         type="C200" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="邮政编码"     type="C10"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="联系电话"     type="C100" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="电子邮箱"     type="C200" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="其他联系方法" type="C100" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="当事人查询账号" type="C30" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="当事人查询密码" type="C30" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="与案实体名录">
    <xsd:sequence>
      <xsd:element name="R" type="与案实体" minOccurs="1" maxOccurs="32768"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="办案期限">
    <xsd:sequence>
      <xsd:element name="审限起始日期"     type="D"  minOccurs="0"/>
      <xsd:element name="审限届满日期"     type="D"  minOccurs="0"/>
      <xsd:element name="立案期限届满日期" type="D"  minOccurs="0"/>
      <xsd:element name="审理结束日期"     type="D"  minOccurs="0"/>
      <xsd:element name="法定立案期限"     type="SN" minOccurs="0"/>
      <xsd:element name="实际立案天数"     type="SN" minOccurs="0"/>
      <xsd:element name="立案超期天数"     type="SN" minOccurs="0"/>
      <xsd:element name="法定审限天数"     type="SN" minOccurs="0"/>
      <xsd:element name="实际审理天数"     type="SN" minOccurs="0"/>
      <xsd:element name="超审限天数"       type="SN" minOccurs="0"/>
      <xsd:element name="超审限倍数"       type="TN" minOccurs="0"/>
      <xsd:element name="超审限原因"       type="TN" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="程序变更">
    <xsd:sequence>
      <xsd:element name="程序变更类型"     type="TN" minOccurs="0"/>
      <xsd:element name="简易转普通日期"   type="D"  minOccurs="0"/>
      <xsd:element name="简易转普通审批人" type="SN" minOccurs="0"/>
      <xsd:element name="简易转普通原因"   type="TN" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:group name="抗诉">
    <xsd:sequence>
      <xsd:element name="抗诉机关"     type="C100" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="提起诉讼日期" type="D"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="提起诉讼材料" type="BB"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="撤回诉讼日期" type="D"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="移送案件日期" type="D"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="上级接收日期" type="D"    minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="抗诉信息">
    <xsd:sequence>
      <xsd:group ref="抗诉" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="上抗诉情况">
    <xsd:sequence>
      <xsd:element name="诉讼范围" type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="诉讼类型" type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="上诉人"   type="BB" minOccurs="0" maxOccurs="1"/>
      <xsd:group    ref="抗诉"               minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:group name="上诉">
    <xsd:sequence>
      <xsd:element name="上诉范围"     type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="上诉日期"     type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="上诉人"       type="BB" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="上诉材料"     type="BB" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="撤回上诉日期" type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="移送案件日期" type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="上级接收日期" type="D"  minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="上诉信息">
    <xsd:sequence>
      <xsd:group ref="上诉" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="诉讼担保情况1">
    <xsd:sequence>
      <xsd:element name="序号_担保"    type="SN"   minOccurs="1" maxOccurs="1"/>
      <xsd:element name="事由"         type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="当事人"       type="SN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="担保方式"     type="N"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="担保人"       type="SN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="与当事人关系" type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="保证金额"     type="M"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="担保物名称"   type="C100" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="担保范围"     type="C200" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="担保期限"     type="SN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="裁定日期"     type="D"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="裁定内容"     type="C200" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="诉讼担保情况">
    <xsd:sequence>
      <xsd:element name="R" type="诉讼担保情况1" minOccurs="0" maxOccurs="32768"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="审判组织成员1">
    <xsd:sequence>
      <xsd:element name="序号_成员" type="SN" minOccurs="1"/>
      <xsd:element name="原成员"    type="SN" minOccurs="0"/>
      <xsd:element name="新成员"    type="SN" minOccurs="0"/>
      <xsd:element name="角色"      type="TN" minOccurs="0"/>
      <xsd:element name="陪审员邀请方"  type="TN" minOccurs="0"/>
      <xsd:element name="变更日期"  type="D"  minOccurs="0"/>
      <xsd:element name="变更原因"  type="TN" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="审判组织成员">
    <xsd:sequence>
      <xsd:element name="R" type="审判组织成员1" minOccurs="0" maxOccurs="32768"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="回避情况1">
    <xsd:sequence>
      <xsd:element name="序号_回避"    type="SN" minOccurs="1"/>
      <xsd:element name="回避类型"     type="TN" minOccurs="0"/>
      <xsd:element name="申请回避日期" type="D"  minOccurs="0"/>
      <xsd:element name="回避原因"     type="N" minOccurs="0"/>
      <xsd:element name="回避人"       type="SN" minOccurs="0"/>
      <xsd:element name="回避申请人"   type="SN" minOccurs="0"/>
      <xsd:element name="回避处理结果" type="TN" minOccurs="0"/>
      <xsd:element name="回避决定人"   type="TN" minOccurs="0"/>
      <xsd:element name="回避决定日期" type="D"  minOccurs="0"/>
      <xsd:element name="回避决定"     type="SN" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="回避情况">
    <xsd:sequence>
      <xsd:element name="R" type="回避情况1" minOccurs="0" maxOccurs="32768"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:group name="成员与回避">
    <xsd:sequence>
      <xsd:element name="审判组织成员" type="审判组织成员" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="回避情况"     type="回避情况"     minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>

  <xsd:complexType name="立案移交和退案1">
    <xsd:sequence>
      <xsd:element name="序号_移交"        type="SN"  minOccurs="1"/>
      <xsd:element name="立案移交庭室标识" type="N"   minOccurs="0"/>
      <xsd:element name="立案移交庭室"     type="C40" minOccurs="0"/>
      <xsd:element name="立案移交人"       type="SN"  minOccurs="0"/>
      <xsd:element name="承办庭标识"       type="N"   minOccurs="0"/>
      <xsd:element name="承办审判庭"       type="C40" minOccurs="0"/>
      <xsd:element name="立案移交日期"     type="D"   minOccurs="0"/>
      <xsd:element name="退案人"           type="SN"  minOccurs="0"/>
      <xsd:element name="退案日期"         type="D"   minOccurs="0"/>
      <xsd:element name="退案原因"         type="TN"  minOccurs="0"/>
      <xsd:element name="退案接收人"       type="SN"  minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="立案移交和退案">
    <xsd:sequence>
      <xsd:element name="R" type="立案移交和退案1" minOccurs="0" maxOccurs="32768"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="法庭使用记录1">
    <xsd:sequence>
      <xsd:element name="序号_法庭使用"            type="SN"  minOccurs="1"/>
      <xsd:element name="法庭用途"                 type="TN"  minOccurs="0"/>
      <xsd:element name="庭次"                     type="TN"  minOccurs="0"/>
      <xsd:element name="开始时间"                 type="DT"  minOccurs="0"/>
      <xsd:element name="结束时间"                 type="DT"  minOccurs="0"/>
      <xsd:element name="地点"                     type="C40" minOccurs="0"/>
      <xsd:element name="公开开庭"                 type="TN"  minOccurs="0"/>
      <xsd:element name="到庭当事人"               type="BB"  minOccurs="0"/>
      <xsd:element name="拒不到庭当事人"           type="BB"  minOccurs="0"/>
      <xsd:element name="无正当理由中途退庭当事人" type="BB"  minOccurs="0"/>
      <xsd:element name="到庭诉讼代理人"           type="BB"  minOccurs="0"/>
      <xsd:element name="出庭作证的证人"           type="BB"  minOccurs="0"/>
      <xsd:element name="辩护人做无罪辩护"         type="TN"  minOccurs="0"/>
      <xsd:element name="拒绝继续辩护或代理"       type="TN"  minOccurs="0"/>
      <xsd:element name="对外宣传"                 type="SN"  minOccurs="0"/>
      <xsd:element name="旁听人数"                 type="SN"  minOccurs="0"/>
      <xsd:element name="人大代表旁听人数"         type="SN"  minOccurs="0"/>
      <xsd:element name="政协委员旁听人数"         type="SN"  minOccurs="0"/>
      <xsd:element name="审委会委员到庭听案"       type="TN"  minOccurs="0"/>
      <xsd:element name="宣判情况"                 type="TN"  minOccurs="0"/>
      <xsd:element name="笔录"                     type="SN"  minOccurs="0"/>
      <xsd:element name="听证裁决情况"             type="TN"  minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="法庭使用记录">
    <xsd:sequence>
      <xsd:element name="R" type="法庭使用记录1" minOccurs="0" maxOccurs="32768"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="法庭使用记录_强医1">
    <xsd:sequence>
      <xsd:element name="序号_法庭使用"            type="SN"  minOccurs="0"/>
      <xsd:element name="法庭用途"                 type="TN"  minOccurs="0"/>
      <xsd:element name="庭次"                     type="TN"  minOccurs="0"/>
      <xsd:element name="开始时间"                 type="DT"  minOccurs="0"/>
